<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="287.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="newSessionButton" minWidth="200.0" mnemonicParsing="false" text="Nouvelle session" textAlignment="CENTER">
               <VBox.margin>
                  <Insets bottom="50.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="loadSessionButton" disable="true" minWidth="200.0" mnemonicParsing="false" text="Charger la session précédente" textAlignment="CENTER">
               <VBox.margin>
                  <Insets bottom="50.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="quitButton" minWidth="200.0" mnemonicParsing="false" text="Quitter l'application" textAlignment="CENTER" />
         </children>
      </VBox>
   </center>
</BorderPane>
